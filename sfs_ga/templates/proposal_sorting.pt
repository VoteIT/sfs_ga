<tal:main xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="sfs_ga">
<div class="listing_block">
    <ul class="tabs_menu" style="float: right;">
        <li class="${sort_tags and 'selected' or None}">
            <a href="${request.resource_url(context, '_set_ai_sorting_tags', query = {'sort': 'tags'})}"
               class="" i18n:translate="">On</a>
        </li>
        <li class="${not sort_tags and 'selected' or None}">
            <a href="${request.resource_url(context, '_set_ai_sorting_tags')}"
               class="" i18n:translate="">Off</a>
        </li>
    </ul>
    <h5 style="float: right;">Sort on tags &nbsp;</h5>

</div><br/>
</tal:main>
